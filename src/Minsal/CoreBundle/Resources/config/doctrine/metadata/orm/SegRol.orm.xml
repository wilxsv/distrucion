<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SegRol" table="seg_rol">
    <unique-constraints>
      <unique-constraint name="seg_rol_id" columns="id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="nombre" type="text" column="nombre" nullable="false"/>
    <field name="fechaCreacion" type="date" column="fecha_creacion" nullable="false"/>
    <field name="fechaModificacion" type="date" column="fecha_modificacion" nullable="true"/>
    <many-to-many field="segUsuarioid" target-entity="SegUsuario" mapped-by="segRolid"/>
  </entity>
</doctrine-mapping>
