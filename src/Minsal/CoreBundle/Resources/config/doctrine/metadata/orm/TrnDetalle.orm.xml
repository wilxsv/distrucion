      <index name="IDX_D232DD2A93154E863CF50ACD" columns="api_establecimientoid,trn_distribucionid"/>
      <index name="IDX_D232DD2AA645FFDB3CF50ACD" columns="api_productoid,trn_distribucionid"/>
