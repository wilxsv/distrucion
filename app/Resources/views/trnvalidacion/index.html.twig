{% extends 'base.html.twig' %}

{% block body %}
    <h1>Trnvalidacions list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Estadoverificado</th>
                <th>Fechamodificacion</th>
                <th>Cantidadprelimimar</th>
                <th>Observacion</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for trnValidacion in trnValidacions %}
            <tr>
                <td><a href="{{ path('validaciones_show', { 'id': trnValidacion.id }) }}">{{ trnValidacion.id }}</a></td>
                <td>{% if trnValidacion.estadoVerificado %}Yes{% else %}No{% endif %}</td>
                <td>{% if trnValidacion.fechaModificacion %}{{ trnValidacion.fechaModificacion|date('Y-m-d') }}{% endif %}</td>
                <td>{{ trnValidacion.cantidadPrelimimar }}</td>
                <td>{{ trnValidacion.observacion }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('validaciones_show', { 'id': trnValidacion.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('validaciones_edit', { 'id': trnValidacion.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('validaciones_new') }}">Create a new trnValidacion</a>
        </li>
    </ul>
{% endblock %}
