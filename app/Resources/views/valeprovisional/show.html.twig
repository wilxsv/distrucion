{% extends 'base.html.twig' %}

{% block body %}
    <h1>Valeprovisional</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ valeProvisional.id }}</td>
            </tr>
            <tr>
                <th>Fechacreacion</th>
                <td>{% if valeProvisional.fechaCreacion %}{{ valeProvisional.fechaCreacion|date('Y-m-d') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('vales_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('vales_edit', { 'id': valeProvisional.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
